	Journal = {J Cereb Blood Flow Metab},
	Keywords = {Alzheimer Disease, metabolism; Amyloid beta-Protein, metabolism; Animals; Apolipoproteins E, metabolism; Biological Transport, Active, physiology; Blood-Brain Barrier; Clusterin, metabolism; Electrophoresis, Polyacrylamide Gel; Enzyme-Linked Immunosorbent Assay; Humans; Iodine Radioisotopes, diagnostic use; Ligands; Mice; Mice, Inbred C57BL; Models, Statistical; Spectrometry, Mass, Electrospray Ionization; Spectrometry, Mass, Matrix-Assisted Laser Desorption-Ionization},
	Language = {eng},
