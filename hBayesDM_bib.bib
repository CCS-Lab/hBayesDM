	Timestamp = {2009.08.15},
	Title = {Learning in a unidimensional absolute identification task.},
	Volume = {11},
	Year = {2004}}

@article{Rouder2008b,
	Abstract = {Visual working memory is often modeled as having a fixed number of
	slots. We test this model by assessing the receiver operating characteristics
	(ROC) of participants in a visual-working-memory change-detection
	task. ROC plots yielded straight lines with a slope of 1.0, a tell-tale
