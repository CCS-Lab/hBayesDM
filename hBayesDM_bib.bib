	Number = {2},
	Owner = {Woo-Young Ahn},
	Pages = {537--545},
	Pii = {S0896-6273(01)00225-2},
	Pmid = {11239442},
	Timestamp = {2009.08.07},
	Title = {Neural activity in the human brain relating to uncertainty and arousal during anticipation.},
