	Month = {Dec},
	Number = {4},
	Owner = {Woo-Young Ahn},
	Pages = {266--277},
	Pmid = {18189000},
	Timestamp = {2009.08.04},
	Title = {Risk prediction and aversion by anterior cingulate cortex.},
	Volume = {7},
	Year = {2007}}

