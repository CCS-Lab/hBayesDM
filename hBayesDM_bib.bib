	Keywords = {Acute Disease; Adult; Antipsychotic Agents, pharmacology/therapeutic use; Attention, drug effects/physiology; Bipolar Disorder, diagnosis/drug therapy/physiopathology; Female; Hospitalization; Humans; Male; Memory, drug effects/physiology; Neuropsychological Tests, statistics /&/ numerical data; Prefrontal Cortex, drug effects/physiopathology; Verbal Learning, drug effects/physiology},
	Language = {eng},
	Medline-Pst = {ppublish},
	Month = {Oct},
	Number = {10},
	Owner = {Young},
