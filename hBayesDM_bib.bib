	Keywords = {Attention; Choice Behavior; Conditioning (Psychology); Decision Support Techniques; Humans; Models, Psychological; Paired-Associate Learning; Pattern Recognition, Visual; Reading; Semantics; Sensory Thresholds; Set (Psychology)},
	Language = {eng},
