	Owner = {Woo-Young Ahn},
	Pages = {25--32},
	Pii = {S0376-8716(99)00010-1},
	Pmid = {10462089},
	Timestamp = {2009.08.06},
	Title = {Excessive discounting of delayed rewards in substance abusers with gambling problems.},
	Volume = {56},
	Year = {1999}}

