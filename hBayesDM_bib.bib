@article{Cowan2004,
	Abstract = {We assessed a hypothesis that working memory capacity should include
	a constant number of separate mental units, or chunks (cf. Miller,
	1956). Because of the practical difficulty of measuring chunks, this
	hypothesis has not been tested previously, despite wide attention
	to Miller's article. We used a training procedure to manipulate the
	strength of associations between pairs of words to be included in
	an immediate serial-recall task. Although the amount of training
	on associations clearly increased the availability of two-item chunks
	and therefore the number of items correct in list recall, the number
	of total chunks recalled (singletons plus two-word chunks) appeared
	to remain approximately constant across association strengths, supporting
	a hypothesis of constant capacity.},
	Author = {Nelson Cowan and Zhijian Chen and Jeffrey N Rouder},
	Doi = {10.1111/j.0956-7976.2004.00732.x},
	Institution = {Department of Psychological Sciences, University of Missouri, Columbia 65211, USA. cowann@missouri.edu},
